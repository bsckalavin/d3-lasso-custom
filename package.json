{
  "name": "@bsckalavin/d3-lasso-custom",
  "version": "1.0.0",
  "description": "A d3 plugin for lasso selecting elements with custom modifications",
  "keywords": [
    "d3",
    "d3-lasso",
    "lasso",
    "custom"
  ],
  "license": "BSD-3-Clause",
  "main": "build/d3-lasso.js",
  "module": "index.js",
  "homepage": "https://github.com/bsckalavin/d3-lasso-custom",
  "repository": {
    "type": "git",
    "url": "https://github.com/bsckalavin/d3-lasso-custom.git"
  },
  "scripts": {
    "build": "rm -rf build && mkdir build && rollup -c",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "npm run build && uglifyjs build/d3-lasso.js -c -m -o build/d3-lasso.min.js",
    "postpublish": "zip -j build/d3-lasso.zip -- LICENSE README.md build/d3-lasso.js build/d3-lasso.min.js"
  },
  "devDependencies": {
    "rollup": "^3.20.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "tape": "^5.6.1",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "d3-dispatch": "^3.0.1",
    "d3-drag": "^3.0.0",
    "d3-selection": "^3.0.0",
    "robust-point-in-polygon": "^1.0.3"
  },
  "type": "module"
}
